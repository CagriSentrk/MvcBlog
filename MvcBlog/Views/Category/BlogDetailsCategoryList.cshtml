@model IEnumerable<EntitiyLayer.Concrete.Category>
   


<div class="sidebar-box">
   
        <h3 class="heading">Kategoriler</h3>
        <ul class="categories">
            @foreach (var item in Model)
            {
                <li><a href="/blog/BlogByCategories/@item.CategoryID">@item.CategoryName<span>@item.CategoryID.</span></a></li>
            }
            </ul> 
    </div>
   